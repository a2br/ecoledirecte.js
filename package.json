{
	"name": "ecoledirecte.js",
	"version": "0.29.1",
	"description": "Good-looking client for EcoleDirecte's private API.",
	"repository": {
		"type": "git",
		"url": "git://github.com/a2br/ecoledirecte.js.git"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [
		"ecoledirecte"
	],
	"scripts": {
		"prepare": "npm run build",
		"start": "npm run build && node dist/index.js",
		"build": "tsc && npm run copy-dts",
		"lint": "eslint",
		"pretty": "prettier -w .",
		"check": "npm run pretty && npm run lint",
		"test": "echo ED demo account unavailable. && exit 0",
		"copy-dts": "copyfiles -u 1 \"lib/**/*.d.ts\" dist"
	},
	"author": "a2br",
	"license": "MIT",
	"devDependencies": {
		"@types/eslint": "^7.2.6",
		"@types/html-to-text": "^8.0.0",
		"@types/jest": "^27.0.0",
		"@types/node": "^16.0.0",
		"@types/node-fetch": "^3.0.3",
		"@typescript-eslint/eslint-plugin": "^4.11.1",
		"@typescript-eslint/parser": "^4.11.1",
		"copyfiles": "^2.4.1",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^8.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.0.1",
		"prettier": "^2.2.1",
		"ts-jest": "^27.0.1",
		"typescript": "^4.2.2"
	},
	"dependencies": {
		"ecoledirecte-api-types": "^0.10.0",
		"html-to-text": "^8.0.0",
		"node-fetch": "^2.6.1"
	}
}
